// 13. Rəqəmləri ədədi tərsinə çevirən və palindrome olub-olmadığını yoxlayan proqram. 
#include <iostream>
using namespace std;
 int main() {
int z,t=0,q;
int z1=z;
cout<<"ededi daxill edin:";
  while(z!=0){
     q=z%10;
     t=t*10+q;
     z=z/10;
 }
 if(z=z1);
 cout<<"polindromdur";
 else if(z!=z1);
 cout<<"polindrom deyil";
    return 0;
}
