#include <iostream>
using namespace std;
void a() {
    cout << "1-1000 araliginda palindrome ededler: ";
    for(int i = 1; i <= 1000; i++) {
        int b = i, rev = 0;
        while(b > 0) {
            c =c * 10 + b % 10;
            b/= 10;
        }
        if(c== i) cout << i << " ";
    }
