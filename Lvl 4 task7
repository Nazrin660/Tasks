#level4_7
#include <iostream>
using namespace std;
void s() {
    cout << "Armstrong ededleri 1-999 araliginda: ";
    for(int i = 1; i < 1000; i++) {
        int sum = 0, b= i;
        while(b > 0) {
            int d = b % 10;
            sum += d * d * d;
            b /= 10;
        }
        if(sum == i)
            cout << i << " ";
    }
    cout << endl;
}
